{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": [
    "*"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-hide",
    "core:app:default",
    "core:path:default",
    "core:webview:allow-create-webview-window",
    "global-shortcut:default",
    "clipboard:default",
    "log:default",
    "opener:default",
    "updater:default",
    "process:default",
    "aptabase:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        { "url": "https://github.com/XueshiQiao/WinPaste" }
      ]
    }
  ]
}
